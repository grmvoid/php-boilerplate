{
	"name": "nulldark/skeleton",
	"type": "project",
	"keywords": [
		"template",
		"skeleton"
	],
	"require": {
		"php": "~8.1.0 || ~8.2.0 || ~8.3.0",
		"vlucas/phpdotenv": "^5.6"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.8",
		"phpstan/phpstan": "^1.10",
		"phpunit/phpunit": "^10.5"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"App\\": "web/src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\Unit\\": "tests/unit/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
