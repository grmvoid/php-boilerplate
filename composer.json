{
    "name": "vendor/project_name",
    "description": ":project_description",
    "type": "project",
    "support": {
        "issues": "https://github.com/:vendor/project_name/issues",
        "source": "https://github.com/:vendor/project_name"
    },
    "require": {
        "php": ">=8.2",
        "ext-mbstring": "*",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.68",
        "phpstan/phpstan": "^2.1.x-dev",
        "phpstan/phpstan-strict-rules": "^2.0.x-dev",
        "phpunit/phpunit": "^11.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\Integration\\": "tests/integration/",
            "App\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.15"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
